{
  "name": "falcon",
  "version": "1.0.15-SNAPSHOT",
  "description": "",
  "scripts": {
    "dev": "npm run prebuild && next src",
    "start": "node .next/production-server/server/index.js",
    "build": "next build src && tsc --project tsconfig.server.json && cp -r src/server/assets .next/production-server/server",
    "prod": "NODE_ENV=production node .next/production-server/server/index.js",
    "test-no-coverage": "jest",
    "test": "jest --coverage",
    "test:watch": "TERM=dumb jest --watch",
    "docker-build": "docker-build",
    "docker-publish": "npm run docker-build && docker-push",
    "lint": "tslint -c tslint.json 'src/**/*.ts*'",
    "lint:fix": "tslint -c tslint.json 'src/**/*.ts*' --fix",
    "prelint": "",
    "prebuild": "npm run lint",
    "clean": "rm -rf build .next src/.next",
    "e2e": "nightwatch --env browserStack",
    "accessibility": "nightwatch --test tests/e2e/testCases/accessibility.js --env browserStack",
    "server": "npm run clean && mkdir build && next build src && nodemon src/server/index.ts",
    "commitmsg": "./git_hooks/commit-msg ${GIT_PARAMS}",
    "precommit": "npm run lint && npm run test-no-coverage"
  },
  "docker-registry": "824691084780.dkr.ecr.us-east-1.amazonaws.com",
  "repository": {
